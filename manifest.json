{
  "name": "look regex",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "description": "An extension to select regex value and go to regex101.com or others",
  "icons": {
    "128": "icons/icon.png"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  }
}